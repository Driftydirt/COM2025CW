<!-- HTML for when a booking fails due the space being already booked -->
<!-- A button for returning to the booking page to try again -->
%div.row
    %div.col.col-sm-3
        = link_to booking_new_path do
            %button.btn.btn-main.col{:type => "button"} 
                %p.button-text= t('.return')
    %div.col.col-sm-6
        %h3= t('.booking_failed')