<p id="notice"><%= notice %></p>

<h1>Car Parks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_parks.each do |car_park| %>
      <tr>
        <td><%= link_to 'Show', car_park %></td>
        <td><%= link_to 'Edit', edit_car_park_path(car_park) %></td>
        <td><%= link_to 'Destroy', car_park, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Park', new_car_park_path %>
